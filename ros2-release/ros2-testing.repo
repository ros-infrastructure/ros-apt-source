[ros2-testing]
name=ROS 2 Testing - $basearch
enabled=0
baseurl=http://packages.ros.org/ros2-testing/rhel/$releasever/$basearch/
gpgcheck=0
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ROS2

[ros2-testing-debug]
name=ROS 2 Testing - $basearch Debug
enabled=0
baseurl=http://packages.ros.org/ros2-testing/rhel/$releasever/$basearch/debug/
gpgcheck=0
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ROS2

[ros2-testing-source]
name=ROS 2 Testing - Source
enabled=0
baseurl=http://packages.ros.org/ros2-testing/rhel/$releasever/SRPMS/
gpgcheck=0
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ROS2
